    <!-- Additional JavaScript -->
    <script>
        // Mobile menu toggle
        function toggleMobileMenu() {
            const mobileNav = document.getElementById('mobile-nav');
            mobileNav.classList.toggle('open');
        }

        // Frosted glass navbar on scroll
        function initializeNavbarScroll() {
            const navbar = document.querySelector('nav');
            let lastScrollTop = 0;
            let ticking = false;
            
            function updateNavbar() {
                const scrollTop = window.pageYOffset || document.documentElement.scrollTop;
                
                if (scrollTop <= 10) {
                    // At the very top - fully opaque
                    navbar.classList.remove('transparent');
                    navbar.classList.remove('scrolled');
                } else if (scrollTop > 10 && scrollTop <= 80) {
                    // Light scroll - frosted glass effect
                    navbar.classList.add('transparent');
                    navbar.classList.remove('scrolled');
                } else {
                    // Deep scroll - solid background
                    navbar.classList.remove('transparent');
                    navbar.classList.add('scrolled');
                }
                
                lastScrollTop = scrollTop;
                ticking = false;
            }
            
            function requestTick() {
                if (!ticking) {
                    requestAnimationFrame(updateNavbar);
                    ticking = true;
                }
            }
            
            window.addEventListener('scroll', requestTick, { passive: true });
        }

        // Initialize navbar scroll effect on page load
        document.addEventListener('DOMContentLoaded', function() {
            initializeNavbarScroll();
        });

        // Close mobile menu when clicking outside
        document.addEventListener('click', function(e) {
            const mobileNav = document.getElementById('mobile-nav');
            const menuButton = document.querySelector('.mobile-menu-button');
            
            if (!mobileNav.contains(e.target) && !menuButton.contains(e.target)) {
                mobileNav.classList.remove('open');
            }
        });

        // Smooth scrolling for navigation links
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    target.scrollIntoView({
                        behavior: 'smooth',
                        block: 'start'
                    });
                }
                // Close mobile menu after navigation
                document.getElementById('mobile-nav').classList.remove('open');
            });
        });

        // Add animation on scroll
        const observerOptions = {
            threshold: 0.1,
            rootMargin: '0px 0px -100px 0px'
        };

        const observer = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    entry.target.classList.add('animate');
                }
            });
        }, observerOptions);

        // Observe all feature cards and industry cards
        document.querySelectorAll('.feature-card, .industry-card, .pricing-card').forEach(el => {
            observer.observe(el);
        });

        // Demo booking functionality is now handled via direct links to lead-generation-form.html

        // Enhanced Demo Chatbot Implementation
        class DemoChatbot {
            constructor(containerId) {
                this.container = document.getElementById(containerId);
                this.chatWindow = document.getElementById('demoChat');
                this.input = document.getElementById('demoInput');
                this.sendButton = document.getElementById('sendButton');
                this.statusDot = document.getElementById('statusDot');
                this.statusText = document.getElementById('statusText');
                this.resetButton = document.getElementById('resetDemo');
                this.loadingOverlay = document.getElementById('demoLoadingOverlay');
                
                this.state = {
                    isProcessing: false,
                    messageCount: 0,
                    currentFlow: 'welcome',
                    userProfile: null,
                    sessionData: {}
                };
                
                this.analytics = {
                    interactions: 0,
                    startTime: Date.now(),
                    messagesSent: 0,
                    flows: []
                };
                
                this.init();
            }
            
            init() {
                this.setupEventListeners();
                this.showWelcomeMessage();
                this.updateStatus('online');
                this.trackAnalytics('demo_started');
            }
            
            setupEventListeners() {
                this.sendButton.addEventListener('click', () => this.handleSendMessage());
                this.input.addEventListener('keypress', (e) => {
                    if (e.key === 'Enter' && !e.shiftKey) {
                        e.preventDefault();
                        this.handleSendMessage();
                    }
                });
                this.input.addEventListener('input', () => this.handleInputChange());
                this.resetButton.addEventListener('click', () => this.resetDemo());
                
                // Auto-resize input
                this.input.addEventListener('input', () => {
                    this.input.style.height = 'auto';
                    this.input.style.height = Math.min(this.input.scrollHeight, 120) + 'px';
                });
            }
            
            async handleSendMessage() {
                const message = this.input.value.trim();
                if (!message || this.state.isProcessing) return;
                
                this.addMessage(message, 'user');
                this.input.value = '';
                this.input.style.height = 'auto';
                this.updateSendButton(true);
                
                this.analytics.messagesSent++;
                this.trackAnalytics('message_sent', { message_count: this.analytics.messagesSent });
                
                await this.processMessage(message);
            }
            
            async processMessage(message) {
                this.setState({ isProcessing: true });
                this.updateStatus('processing');
                
                // Show typing indicator
                await this.delay(500);
                this.showTypingIndicator();
                
                // Simulate processing time
                await this.delay(1200 + Math.random() * 800);
                
                this.removeTypingIndicator();
                
                // Generate response based on message content and current flow
                const response = await this.generateResponse(message);
                
                this.addMessage(response.text, 'bot');
                
                if (response.quickActions) {
                    await this.delay(800);
                    this.showQuickActions(response.quickActions);
                }
                
                if (response.followUp) {
                    await this.delay(1000);
                    this.showFollowUpOptions(response.followUp);
                }
                
                this.setState({ isProcessing: false });
                this.updateStatus('online');
                this.updateSendButton(false);
            }
            
            async generateResponse(message) {
                const lowerMessage = message.toLowerCase();
                
                // Intent detection
                if (lowerMessage.includes('appointment') || lowerMessage.includes('schedule')) {
                    this.analytics.flows.push('appointment');
                    return {
                        text: "I'd be happy to help you schedule an appointment! I can see you're a returning patient. Let me check your available options.",
                        followUp: ["General Checkup", "Follow-up Visit", "Urgent Care", "Specialist Referral"]
                    };
                }
                
                if (lowerMessage.includes('prescription') || lowerMessage.includes('refill') || lowerMessage.includes('medication')) {
                    this.analytics.flows.push('prescription');
                    return {
                        text: "I can help with prescription refills. I found your recent prescriptions in our system. Which medication would you like to refill?",
                        followUp: ["Blood Pressure Medication", "Diabetes Medication", "Allergy Medication", "Other Medication"]
                    };
                }
                
                if (lowerMessage.includes('insurance') || lowerMessage.includes('coverage') || lowerMessage.includes('copay')) {
                    this.analytics.flows.push('insurance');
                    return {
                        text: "I can help with insurance questions. I see you have Blue Cross Blue Shield coverage. What would you like to know?",
                        followUp: ["Coverage Details", "Claims Status", "Copay Information", "Prior Authorization"]
                    };
                }
                
                if (lowerMessage.includes('hours') || lowerMessage.includes('open') || lowerMessage.includes('closed')) {
                    return {
                        text: "Our clinic hours are Monday-Friday 8:00 AM to 6:00 PM, and Saturday 9:00 AM to 2:00 PM. We're closed on Sundays. For urgent matters after hours, please call our emergency line."
                    };
                }
                
                if (lowerMessage.includes('emergency') || lowerMessage.includes('urgent')) {
                    return {
                        text: "For medical emergencies, please call 911 immediately. For urgent but non-emergency care, you can reach our after-hours line at (555) 123-URGENT or visit our urgent care center.",
                        quickActions: ["Call Emergency Line", "Find Urgent Care Location"]
                    };
                }
                
                // Default responses for demonstration
                const responses = [
                    {
                        text: "I understand you'd like to know more about our services. Our AI assistant can help with appointments, prescriptions, insurance questions, and general medical inquiries. What would you like help with today?",
                        quickActions: ["Schedule Appointment", "Refill Prescription", "Insurance Questions"]
                    },
                    {
                        text: "That's a great question! Our AI system is designed to handle complex healthcare workflows. In a real implementation, I would access your patient records, insurance information, and provider schedules to give you specific answers.",
                        followUp: ["See Live Demo", "Learn More About Features"]
                    }
                ];
                
                return responses[Math.floor(Math.random() * responses.length)];
            }
            
            addMessage(text, sender, timestamp = null) {
                const messageDiv = document.createElement('div');
                messageDiv.className = `chat-message ${sender}-message`;
                
                const time = timestamp || new Date().toLocaleTimeString([], {hour: '2-digit', minute:'2-digit'});
                const avatar = sender === 'user' ? 'üë§' : 'ü§ñ';
                
                messageDiv.innerHTML = `
                    <div class="message-avatar">${avatar}</div>
                    <div class="message-content">
                        <div class="message-text">${text}</div>
                        <div class="message-time">${time}</div>
                    </div>
                `;
                
                this.chatWindow.appendChild(messageDiv);
                this.scrollToBottom();
                this.state.messageCount++;
            }
            
            showTypingIndicator() {
                const typingDiv = document.createElement('div');
                typingDiv.className = 'chat-message bot-message typing-indicator';
                typingDiv.innerHTML = `
                    <div class="message-avatar">ü§ñ</div>
                    <div class="message-content">
                        <div class="message-text">
                            <div class="typing-dots">
                                <div class="typing-dot"></div>
                                <div class="typing-dot"></div>
                                <div class="typing-dot"></div>
                            </div>
                        </div>
                    </div>
                `;
                
                this.chatWindow.appendChild(typingDiv);
                this.scrollToBottom();
            }
            
            removeTypingIndicator() {
                const typingIndicator = this.chatWindow.querySelector('.typing-indicator');
                if (typingIndicator) {
                    typingIndicator.remove();
                }
            }
            
            showQuickActions(actions) {
                const actionsDiv = document.createElement('div');
                actionsDiv.className = 'quick-actions';
                
                actions.forEach(action => {
                    const button = document.createElement('button');
                    button.className = 'quick-action';
                    button.innerHTML = `<span class="quick-action-icon">‚ö°</span>${action}`;
                    button.onclick = () => this.handleQuickAction(action);
                    actionsDiv.appendChild(button);
                });
                
                this.chatWindow.appendChild(actionsDiv);
                this.scrollToBottom();
            }
            
            showFollowUpOptions(options) {
                const followUpDiv = document.createElement('div');
                followUpDiv.className = 'follow-up-options';
                
                options.forEach(option => {
                    const button = document.createElement('button');
                    button.textContent = option;
                    button.onclick = () => this.handleFollowUp(option);
                    followUpDiv.appendChild(button);
                });
                
                this.chatWindow.appendChild(followUpDiv);
                this.scrollToBottom();
            }
            
            async handleQuickAction(action) {
                this.disableQuickActions();
                this.addMessage(action, 'user');
                
                if (action === 'Call Emergency Line') {
                    await this.delay(800);
                    this.addMessage("I've initiated a call to our emergency line. In a real implementation, this would connect you directly or provide the emergency contact information.", 'bot');
                } else {
                    await this.processMessage(action);
                }
            }
            
            async handleFollowUp(option) {
                this.disableFollowUpButtons();
                this.addMessage(option, 'user');
                
                await this.delay(1000);
                this.showTypingIndicator();
                await this.delay(1500);
                this.removeTypingIndicator();
                
                if (option.includes('Demo') || option.includes('Learn More')) {
                    this.addMessage("Perfect! I'd love to show you how our AI assistant can transform your practice. Our live demo will show you real patient scenarios and workflow automation.", 'bot');
                    
                    await this.delay(1000);
                    const ctaDiv = document.createElement('div');
                    ctaDiv.className = 'demo-cta';
                    ctaDiv.innerHTML = `
                        <button onclick="window.location.href='lead-generation-form.html'" style="
                            background: var(--conversion-red);
                            color: white;
                            border: none;
                            border-radius: 0.5rem;
                            padding: 1rem 2rem;
                            font-size: 1rem;
                            font-weight: 600;
                            cursor: pointer;
                            width: 100%;
                            margin-top: 1rem;
                            transition: all 0.3s ease;
                        " onmouseover="this.style.background='var(--conversion-red-dark)'" onmouseout="this.style.background='var(--conversion-red)'">
                            üöÄ Get Your Free Live Demo
                        </button>
                    `;
                    this.chatWindow.appendChild(ctaDiv);
                    this.trackAnalytics('cta_shown');
                } else {
                    this.addMessage("Excellent choice! I've started processing that for you. In a real implementation, I would now access our scheduling system, update your patient records, and send confirmations. This demonstrates the power of automated healthcare workflows.", 'bot');
                }
                
                this.scrollToBottom();
            }
            
            disableQuickActions() {
                this.chatWindow.querySelectorAll('.quick-action').forEach(btn => {
                    btn.disabled = true;
                    btn.style.opacity = '0.6';
                    btn.style.cursor = 'not-allowed';
                });
            }
            
            disableFollowUpButtons() {
                this.chatWindow.querySelectorAll('.follow-up-options button').forEach(btn => {
                    btn.disabled = true;
                    btn.style.opacity = '0.6';
                    btn.style.cursor = 'not-allowed';
                });
            }
            
            showWelcomeMessage() {
                setTimeout(() => {
                    this.addMessage("Hi! Welcome to Sunshine Medical Center. I'm your AI assistant, and I can help you with appointments, prescriptions, insurance questions, and more. How can I assist you today?", 'bot');
                    
                    setTimeout(() => {
                        this.showQuickActions(["Schedule Appointment", "Refill Prescription", "Insurance Questions"]);
                    }, 1000);
                }, 500);
            }
            
            resetDemo() {
                this.showLoadingOverlay("Resetting demo...");
                
                setTimeout(() => {
                    this.chatWindow.innerHTML = '';
                    this.input.value = '';
                    this.input.style.height = 'auto';
                    this.state = {
                        isProcessing: false,
                        messageCount: 0,
                        currentFlow: 'welcome',
                        userProfile: null,
                        sessionData: {}
                    };
                    
                    this.hideLoadingOverlay();
                    this.updateStatus('online');
                    this.showWelcomeMessage();
                    this.trackAnalytics('demo_reset');
                }, 1000);
            }
            
            showLoadingOverlay(text = "Processing...") {
                this.loadingOverlay.querySelector('.demo-loading-text').textContent = text;
                this.loadingOverlay.style.display = 'flex';
            }
            
            hideLoadingOverlay() {
                this.loadingOverlay.style.display = 'none';
            }
            
            updateStatus(status) {
                const statusMap = {
                    online: { text: 'AI Assistant Online', class: '' },
                    processing: { text: 'Processing...', class: 'processing' },
                    error: { text: 'Connection Error', class: 'error' }
                };
                
                const statusInfo = statusMap[status] || statusMap.online;
                this.statusText.textContent = statusInfo.text;
                this.statusDot.className = `status-dot ${statusInfo.class}`;
            }
            
            updateSendButton(isLoading) {
                if (isLoading) {
                    this.sendButton.classList.add('loading');
                    this.sendButton.disabled = true;
                    this.input.disabled = true;
                } else {
                    this.sendButton.classList.remove('loading');
                    this.sendButton.disabled = false;
                    this.input.disabled = false;
                    this.input.focus();
                }
            }
            
            handleInputChange() {
                const hasText = this.input.value.trim().length > 0;
                this.sendButton.style.opacity = hasText ? '1' : '0.6';
            }
            
            scrollToBottom() {
                this.chatWindow.scrollTop = this.chatWindow.scrollHeight;
            }
            
            setState(newState) {
                this.state = { ...this.state, ...newState };
            }
            
            delay(ms) {
                return new Promise(resolve => setTimeout(resolve, ms));
            }
            
            trackAnalytics(event, data = {}) {
                this.analytics.interactions++;
                const eventData = {
                    event,
                    timestamp: Date.now(),
                    sessionDuration: Date.now() - this.analytics.startTime,
                    messageCount: this.state.messageCount,
                    ...data
                };
                
                // Track in console for demo purposes
                console.log('Demo Analytics:', eventData);
                
                // In production, this would send to your analytics service
                if (typeof gtag !== 'undefined') {
                    gtag('event', 'demo_interaction', {
                        event_category: 'demo',
                        event_label: event,
                        value: this.analytics.interactions
                    });
                }
            }
        }
        
        // Initialize the demo chatbot
        let demoChatbot;
        document.addEventListener('DOMContentLoaded', function() {
            if (document.getElementById('demoContainer')) {
                demoChatbot = new DemoChatbot('demoContainer');
            }
        });
    </script>
</body>
</html>